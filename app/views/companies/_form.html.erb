<%= form_for @company do |f| %>
  <p>
    <%= f.label :company_name %>
    <%= f.text_field :company_name %>
  </p>
  <p>
    <%= f.label :company_hq %>
    <%= f.text_area :company_hq %>
  </p>
  <p>
    <%= f.label :ceo %>
    <%= f.text_field :ceo %>
  </p>
  <p>
    <%= f.label :employees %>
    <%= f.text_area :employees %>
  </p>
  <p>
    <%= f.submit %>
  </p>
  <p>
  	<%= link_to "Back", companies_path %>
  </p>
<% end %>
